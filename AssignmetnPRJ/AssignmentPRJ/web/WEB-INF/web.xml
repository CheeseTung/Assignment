<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RecordController</servlet-name>
        <servlet-class>Controller.RecordController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>chooseType</servlet-name>
        <servlet-class>login.chooseType</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sign</servlet-name>
        <servlet-class>login.sign</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signCustomer</servlet-name>
        <servlet-class>login.signCustomer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginCustomer</servlet-name>
        <servlet-class>login.loginCustomer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeAdmin</servlet-name>
        <servlet-class>HomePage.HomeAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeCustomer</servlet-name>
        <servlet-class>HomePage.HomeCustomer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RecordController</servlet-name>
        <url-pattern>/recordView</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>chooseType</servlet-name>
        <url-pattern>/chooseType</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sign</servlet-name>
        <url-pattern>/sign</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signCustomer</servlet-name>
        <url-pattern>/signCustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginCustomer</servlet-name>
        <url-pattern>/loginCustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeAdmin</servlet-name>
        <url-pattern>/HomeAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeCustomer</servlet-name>
        <url-pattern>/HomeCustomer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
